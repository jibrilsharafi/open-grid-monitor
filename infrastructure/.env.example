# Open Grid Monitor Infrastructure Configuration
# Copy this file to .env and update the values
# NOTE: All passwords must be at least 8 characters long

# MQTT Credentials
MQTT_USERNAME=open_grid_monitor
MQTT_PASSWORD=your_secure_mqtt_password_8chars_min

# InfluxDB Credentials
INFLUXDB_ADMIN_USERNAME=admin
INFLUXDB_ADMIN_PASSWORD=your_secure_influxdb_password_8chars_min
INFLUXDB_ORG=homelab
INFLUXDB_BUCKET=open_grid_monitor

# InfluxDB Token (automatically generated by setup script)
INFLUXDB_TOKEN=your_influxdb_token_here

# Grafana Credentials
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password_8chars_min
